@model IEnumerable<FlyHigh.Models.Steward>

@{
    ViewBag.Title = "Index";
}

<h1>Steward</h1>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.StewardName)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.StewardName)
                </td>
                <td>
                    @Html.ActionLink("Edit", "Edit", new { id = item.StewardId }) |
            @Html.ActionLink("Details", "Details", new { id = item.StewardId }) |
            @Html.ActionLink("Delete", "Delete", new { id = item.StewardId })
                </td>
            </tr>
        }
    </tbody>

</table>
